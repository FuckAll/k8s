Version": "v1",
    "kind": "ReplicationController",
    "metadata": {
        "name": "story"
    },
    "spec": {
        "replicas": 1,
        "selector": {
            "app": "story"
        },
        "template": {
            "metadata": {
                "labels": {
                    "app": "story"
                }
            },
            "spec": {
                "containers": [
                    {
                        "name": "story",
                        "image": "reg.17mei.top/story/cdc7233f-story:v1.2",
                        "imagePullPolicy": "Always",
                        "ports": [
                            {
                                "containerPort": 3000,
                                "name": "tcp"
                            }
                        ],
                        "env": [
                            {
                                "name": "CH",
                                "value": "192.168.118.182"
                            },
                            {
                                "name": "CP",
                                "value": 8600
                            },
                            {
                                "name": "SS",
                                "value": ".service.dc1.consul"
                            },
                            {
                                "name": "DH",
                                "value": "192.168.9.243"
                            },
                            {
                                "name": "DN",
                                "value": "meidb"
                            },
                            {
                                "name": "DP",
                                "value": 5432
                            },
                            {
                                "name": "DPWD",
                                "value": "wothing"
                            },
                            {
                                "name": "DU",
                                "value": "postgres"
                            },
                            {
                                "name": "P",
                                "value": 3000
                            }
                        ]
                    }
                ]
            }
        }
    }
}
